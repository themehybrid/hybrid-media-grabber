{
	"name": "themehybrid/hybrid-media-grabber",
	"description": "Hybrid Media Grabber is a script for pulling media either from the post content or attached to the post, an addon for the Hybrid Core framework.",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress"
	],
	"authors": [
		{
			"name": "Theme Hybrid",
			"email": "themehybrid@gmail.com",
			"homepage": "https://themehybrid.com",
			"role": "Developer"
		}
	],
	"homepage": "https://github.com/themehybrid/hybrid-media-grabber",
	"support": {
		"issues": "https://github.com/themehybrid/hybrid-media-grabber/issues",
		"forum": "https://themehybrid.com/board/forums/hybrid-core"
	},
	"require": {
		"php": ">=7.4",
		"themehybrid/hybrid-contracts": "^1.0 || ^2.0"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Hybrid\\Media\\Grabber\\": "src/"
		},
		"files": [
			"src/functions-helpers.php"
		]
	},
	"config": {
		"sort-packages": true
	}
}
